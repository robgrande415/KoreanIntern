  keywords = {H$^{\&infin;}$ control; Popov criterion; control system synthesis;
	convergence; iterative methods; matrix algebra; robust control; H$_{\&infin;}$
	control; Popov control; convergence; iterative algorithm; linear
	matrix inequality; robust control},
  ty = {CONF},
