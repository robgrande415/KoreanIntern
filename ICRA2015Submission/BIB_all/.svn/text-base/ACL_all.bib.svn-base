@INPROCEEDINGS{MTL,
  author = {S. Karaman and E. Frazzoli},
  title = {Vehicle Routing Problem with Metric Temporal Logic Specifications},
  booktitle = CDC,
  year = {2008},
  abstract = {This paper proposes a novel version of the Vehicle Routing Problem
	(VRP), in which not all customers have to be serviced nor all the
	vehicles have to be employed. Instead, feasible solutions of the
	VRP instance are forced to satisfy a set of complex high-level tasks
	given as a Metric Temporal Logic (MTL) specification. For the resulting
	Vehicle Routing Problem with Metric Temporal Logic Specifications
	(VRPMTL), a Mixed-Integer Linear Programming (MILP) based algorithm
	is provided that solves the problem to optimality. Examples for optimal
	multi-{UAV} mission planning is provided where MTL is used as a high
	level language to specify complex mission tasks.},
