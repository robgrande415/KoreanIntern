@INPROCEEDINGS{Cas03,
  author = {Castanon, D. A. and Wu, C.},
  title = {Distributed algorithms for dynamic reassignment},
  booktitle = cdc,
  year = {9-12 Dec. 2003},
  volume = {1},
  pages = {13--18},
  annote = {In this paper, we consider the problem of task partitioning among
	members of a team of cooperating agents in response to discovery
	of new tasks or potential failures of some agents. We assume that
	information about new targets or agent failures is received by individual
	team members, and communicated asynchronously with delays to the
	rest of the team. These delays create potential differences in information
	across the team of agents. We describe an asynchronous approach to
	coordinating the team response, where individual agents compute modifications
	to assignments based on local information. We show that the asynchronous
	algorithms converge to the same optimal assignments in the presence
	of arbitrary finite communication delays as a centralized information
	approach. We extend the asynchronous protocol to the solution of
	a class of stochastic dynamic resource assignment problems, and show
	asynchronous convergence of the resulting algorithms. Simulations
	illustrate the delays in computing an optimal assignment of tasks
	in response to dynamic events.},
  bdsk-url-online-1 = {10.1109/CDC.2003.1272528},
  date-added = {2010-07-26 09:49:55 -0400},
  date-modified = {2011-03-15 19:40:39 -0400},
  isbn = {0191-2216},
  journal = cdc,
  journal1 = {Decision and Control, 2003. Proceedings. 42nd IEEE Conference on DOI
	- 10.1109/CDC.2003.1272528},
  keywords = {convergence of numerical methods; distributed algorithms; optimisation;
	arbitrary finite communication delays; asynchronous algorithms; asynchronous
	convergence; asynchronous protocol; centralized information; cooperating
	agents; distributed algorithms; dynamic events; dynamic reassignment;
	optimal assignments; stochastic dynamic resource assignment problems;
	task partitioning problem; team response coordination},
  ty = {CONF},
  url-online = {10.1109/CDC.2003.1272528},
  vo = {1},
  year1 = {2003}
}

