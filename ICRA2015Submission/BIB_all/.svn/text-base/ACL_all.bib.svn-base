@ARTICLE{Cortes.Martinez.ea:06,
  author = {J. Cort{\'e}s and S. Mart{\'\i}nez and F. Bullo},
  title = {Robust Rendezvous for Mobile Autonomous Agents via Proximity Graphs
	in Arbitrary Dimensions},
  journal = {IEEE Trans. on Automatic Control},
  year = {2005},
  month = {July},
  note = {To appear},
  abstract = {This paper presents coordination algorithms for groups of robotic
	agents. The objective of the proposed algorithms is to achieve rendezvous,
	that is, agreement over the location of the agents in the network.
	We provide analysis and design results for multi-agent networks in
	arbitrary dimensions under mild requirements on the switching and
	failing communication topology. The novel correctness proof relies
	on proximity graphs and their properties and on a general LaSalle
	Invariance Principle for nondeterministic discrete-time dynamical
	systems.},
  date-added = {2010-07-27 20:58:09 -0400},
