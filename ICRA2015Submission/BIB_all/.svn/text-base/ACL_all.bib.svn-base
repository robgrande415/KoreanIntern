@ARTICLE{Chapelle:EtAl:02,
  author = {O. Chapelle and V. Vapnik and O. Bousquet and S. Mukherjee},
  title = {Choosing Multiple Parameters for Support Vector Machines},
  journal = jmlr,
  year = {2002},
  volume = {46},
  pages = {131--159},
  number = {1},
  abstract = {The problem of automatically tuning multiple parameters for pattern
	recognition Support Vector Machines (SVMs) is considered. This is
	done by minimizing some estimates of the generalization error of
	SVMs using a gradient descent algorithm over the set of parameters.
	Usual methods for choosing parameters, based on exhaustive search
	become intractable as soon as the number of parameters exceeds two.
	Some experimental results assess the feasibility of our approach
	for a large number of parameters (more than 100) and demonstrate
	an improvement of generalization performance.},
